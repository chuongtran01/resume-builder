# AI Configuration
# Copy this file to .env and fill in your actual values

# Required: Gemini API Key
GEMINI_API_KEY=your-api-key-here

# Optional: Default AI Provider (default: gemini)
DEFAULT_AI_PROVIDER=gemini

# Optional: Gemini Model (default: gemini-2.5-pro)
# Options: gemini-2.5-pro, gemini-3-flash-preview
GEMINI_MODEL=gemini-2.5-pro

# Optional: Gemini Temperature 0.0-1.0 (default: 0.7)
GEMINI_TEMPERATURE=0.7

# Optional: Maximum tokens to generate (default: 2000)
GEMINI_MAX_TOKENS=2000

# Optional: Request timeout in milliseconds (default: 30000)
GEMINI_TIMEOUT=30000

# Optional: Maximum retry attempts (default: 3)
GEMINI_MAX_RETRIES=3

# Optional: Enhancement mode (default: sequential)
# Options: sequential, agent
ENHANCEMENT_MODE=sequential
